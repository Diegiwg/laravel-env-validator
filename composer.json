{
  "name": "melihovv/laravel-env-validator",
  "description": "Laravel Validator for the .env file",
  "license": "MIT",
  "keywords": [
    "env",
    "env-validator",
    "validation",
    "laravel"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Alexander Melihov",
      "email": "amelihovv@ya.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Melihovv\\LaravelEnvValidator\\": "src/LaravelEnvValidator"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Melihovv\\LaravelEnvValidator\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0",
    "illuminate/validation": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "scripts": {
    "phpunit": "phpunit --dont-report-useless-tests"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Melihovv\\LaravelEnvValidator\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
